        <Button fx:id="btnSelectComponent" layoutY="276.0" maxWidth="75.0" minWidth="75.0" mnemonicParsing="false" onAction="#onSelectPressed" prefWidth="75.0" text="Add" AnchorPane.leftAnchor="14.0" />
